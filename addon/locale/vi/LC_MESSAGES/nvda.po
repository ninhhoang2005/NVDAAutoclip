#: addon/globalPlugins/autoclip/__init__.py:82
#: addon/globalPlugins/autoclip/__init__.py:111
#: addon/globalPlugins/autoclip/__init__.py:165 buildVars.py:23
msgid "Autoclip"
msgstr "Autoclip"

#: addon/globalPlugins/autoclip/__init__.py:90
msgid "&Automatic clipboard reading"
msgstr "&Tự động đọc clipboard"

#: addon/globalPlugins/autoclip/__init__.py:90
#: addon/globalPlugins/autoclip/__init__.py:110
msgid "Toggles Autoclip, Automatic clipboard reading."
msgstr "Bật/tắt Autoclip, Tự động đọc clipboard."

#: addon/globalPlugins/autoclip/__init__.py:139
msgid "Enabled Automatic Clipboard Reading."
msgstr "Đã bật tính năng đọc Clipboard tự động."

#: addon/globalPlugins/autoclip/__init__.py:143
msgid "Disabled Automatic Clipboard Reading."
msgstr "Đã tắt Tự động đọc Clipboard."

#: addon/globalPlugins/autoclip/__init__.py:170
msgid "Always &Interrupt before speaking the clipboard"
msgstr "Luôn &Ngắt trước khi đọc bảng nhớ tạm"

#: addon/globalPlugins/autoclip/__init__.py:175
msgid ""
"&Remember automatic clipboard reading after NVDA restart. Required for use in "
"configuration profiles"
msgstr ""
"&Ghi nhớ việc đọc clipboard tự động sau khi khởi động lại NVDA. Cần thiết để sử "
"dụng trong hồ sơ cấu hình"

#: buildVars.py:26
msgid ""
"Automatically read the contents of the clipboard when a change is detected. Press "
"NVDA + Shift + Control + K to toggle."
msgstr ""
"Tự động đọc nội dung của clipboard khi phát hiện thay đổi.Nhấn NVDA + Shift + "
"Control + K để chuyển đổi."

#: addon\globalPlugins\autoclip\__init__.py:149
msgid "Toggles if Autoclip should interrupt speech before speaking the clipboard"
msgstr "Chuyển đổi nếu Autoclip làm gián đoạn lời nói trước khi nói vào bảng nhớ tạm"

#: addon\globalPlugins\autoclip\__init__.py:156
msgid "Enabled \"Interrupt before speaking the clipboard\""
msgstr "Đã bật \"Ngắt trước khi nói bảng nhớ tạm\""

#: addon\globalPlugins\autoclip\__init__.py:159
msgid "Disabled \"Interrupt before speaking the clipboard\""
msgstr "Đã tắt \"Ngắt trước khi nói bảng nhớ tạm\""

#: addon\globalPlugins\autoclip\__init__.py:242
msgid "&Show in the NVDA tools menu"
msgstr "&Hiển thị trong menu công cụ của NVDA"

#. Advanced settings
#: addon/globalPlugins/autoclip/__init__.py:273
msgid "Advanced Settings"
msgstr "Cài đặt nâng cao"

#: addon/globalPlugins/autoclip/__init__.py:279
msgid ""
"Split text above this length to segments spoken separately (number under 100 to "
"disable):"
msgstr ""
"Tách văn bản trên độ dài này thành các đoạn được đọc riêng (số dưới 100 sẽ tắt):"

#: addon/globalPlugins/autoclip/__init__.py:288
msgid "When splitting text, try to split segments at word boundaries"
msgstr "Khi tách văn bản, hãy cố gắng tách các đoạn ở ranh giới từ"

#: addon/globalPlugins/autoclip/__init__.py:293
msgid "Maximum text length to speak (characters):"
msgstr "Độ dài văn bản tối đa để nói (ký tự):"

#: addon/globalPlugins/autoclip/__init__.py:301
msgid ""
"Debounce Delay to not speaking a clipboard update with the same text "
"(milliseconds) (0 to disable and never filter extra equivalent clipboard updates) "
"(-1 to never speak an equivalent clipboard update:"
msgstr ""
"Gỡ bỏ độ trễ để không đọc bản cập nhật bộ nhớ tạm có cùng văn bản (mili giây) (0 "
"để tắt và không bao giờ lọc các bản cập nhật bộ nhớ tạm tương đương bổ sung) (-1 "
"để không bao giờ đọc bản cập nhật bộ nhớ tạm tương đương:"

#: addon/globalPlugins/autoclip/__init__.py:310
msgid ""
"Minimum delay between speech interrupts (milliseconds) (0 to disable and to always "
"interrupt):"
msgstr ""
"Độ trễ tối thiểu giữa các lần ngắt giọng nói (mili giây) (0 để tắt và luôn ngắt):"

#: addon/globalPlugins/autoclip/__init__.py:318
msgid "Restore advanced settings to &defaults"
msgstr "Khôi phục cài đặt nâng cao về &mặc định"

#. Add-on summary, usually the user visible name of the addon.
#. Translators: Summary for this add-on
#. to be shown on installation and add-on information found in Add-ons Manager.
#: buildVars.py:23
msgid "Autoclip, automatically speak clipboard content when it changes"
msgstr "Autoclip, tự động đọc nội dung clipboard khi nó thay đổi"
